<project 
  xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                  PARENT                                 -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <parent>
    <groupId>com.github.sebhoss</groupId>
    <artifactId>java-parent</artifactId>
    <version>1.0.0</version>
  </parent>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                               INFORMATIONS                              -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <groupId>com.github.sebhoss.contract</groupId>
  <artifactId>annotated-contracts</artifactId>
  <version>1.0.0</version>
  <packaging>pom</packaging>
  <inceptionYear>2012</inceptionYear>
  <url>http://github.com/sebhoss/annotated-contracts</url>
  <name>Annotated-Contracts</name>
  <description>Annotation-based design by contract library.</description>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                 MODULES                                 -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <modules>
    <module>contract-annotations</module>
    <module>contract-testutils</module>
    <module>contract-aop</module>
    <module>contract-verifier</module>
    <module>contract-paranamer</module>
    <module>contract-el</module>
    <module>contract-juel</module>
    <module>contract-javascript</module>
    <module>contract-jexl</module>
    <module>contract-mvel</module>
    <module>contract-ognl</module>
    <module>contract-spring</module>
    <module>contract-guice</module>
    <module>contract-guice-example</module>
  </modules>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                PROPERTIES                               -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <properties>
    <scm.connection>scm:git:git://github.com/sebhoss/annotated-contracts.git</scm.connection>
    <scm.developerConnection>scm:git:git@github.com:sebhoss/annotated-contracts.git</scm.developerConnection>
    <scm.url>http://github.com/sebhoss/annotated-contracts</scm.url>
    <issueManagement.url>http://github.com/sebhoss/annotated-contracts/issues</issueManagement.url>
  </properties>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                  SOURCE                                 -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <scm>
    <connection>${scm.connection}</connection>
    <developerConnection>${scm.developerConnection}</developerConnection>
    <tag>master</tag>
    <url>${scm.url}</url>
  </scm>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                  ISSUES                                 -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <issueManagement>
    <system>GitHub</system>
    <url>${issueManagement.url}</url>
  </issueManagement>

</project>